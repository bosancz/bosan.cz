{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    },
    {
      "name": "vendor",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css",
          "https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700",
          "https://use.fontawesome.com/releases/v5.0.10/css/all.css"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "/api",
        "/api/config",
        "/api/me/user",
        "/api/events/program",
        "/api/albums/recent?limit=3"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "7d"
      },
      "navigationUrls": [
        "/**", 
        "!/api/events/**/accounting/template"
      ]
    }
  ]
}
