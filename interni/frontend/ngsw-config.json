{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    },
    {
      "name": "vendor",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": ["https://fonts.googleapis.com/css?family=Roboto:400,700&display=swap"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": ["/api", "/api/me/user"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "7d"
      }
    },
    {
      "name": "database",
      "urls": ["/api/members", "/api/members/*"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "7d"
      }
    }
  ]
}
